{
  "name": "listpkgs.nuros.front-end",
  "private": true,
  "version": "3.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint:js": "eslint src/ --ext .ts,.tsx --fix",
    "lint:scss": "stylelint src/styles/ --fix",
    "lint": "pnpm lint:js && pnpm lint:scss",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "check-format": "prettier --check \"src/**/*.{ts,tsx,css,scss}\"",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug"
  },
  "dependencies": {
    "prismjs": "^1.29.0",
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@playwright/test": "^1.48.2",
    "@types/node": "^24.10.1",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.33",
    "@typescript-eslint/parser": "^8.0.0-alpha.33",
    "eslint": "^9.4.0",
    "eslint-plugin-solid": "^0.14.3",
    "prettier": "^3.3.3",
    "sass": "^1.83.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.0.0-alpha.33",
    "vite": "^7.3.1",
    "vite-plugin-solid": "^2.11.10"
  }
}
